SkillToys directory landing Page index.ejs

<table id="list">
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Get specs</th>
    </tr>
  </thead>
  <tbody>
    <% skilltoys.forEach(function(s) { %>
      <tr>
        <td><%= s.brand %></td>
        <td><%= s.model %></td>
        <td><a href="/skilltoys/<%= s._id %>">Check it out, Yo!</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>


