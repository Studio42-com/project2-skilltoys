<%- include('../partials/header') %>

  <!-- start cast list -->
  <div>Companies</div>
  
  <!-- end cast list -->
<form id="addCompany" action="/skilltoys/<%= skilltoy._id %>/companies" method="POST">
  <select name="companyId">
    <% companies.forEach(function(p) { %>
      <option value="<%= p._id %>"><%= p.company %></option>
    <% }); %>
  </select>
  <button type="submit">Company</button>
</form>



<%- include('../partials/footer') %>
